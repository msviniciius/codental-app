# Ignore bundler config
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore database files
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore Sprockets assets files
/public/assets
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
.byebug_history

# Ignore system cache
/.sass-cache
/.cache

# Ignore IDE files
*.swp
*.idea/
*.vscode/
.DS_Store

# Ignore environment config
/config/master.key
/config/credentials.yml.enc
